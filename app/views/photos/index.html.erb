<h1>List of Photos </h1>
<% @list_of_all_my_photos.each do |the_photo| %>
<p>
  <%= the_photo.caption %>
</p>
<p>
  <img src = "<%= the_photo.source%>">
</p>


<p>
  <a href="/photos/<%= the_photo.id %>" class="btn btn-default btn-block">  Details</a>
  <a href="/photos/<%= the_photo.id %>/edit" class="btn btn-default btn-block">  Edit</a>
  <a href="/delete_photo/<%= the_photo.id %>" class="btn btn-default btn-block">  Delete</a>
</p>

<% end %>
